{
    "manifest_version": 3,
    "name": "Timezone Spoofer",
    "version": "1.4",
    "permissions": [
      "storage",
      "scripting",
      "debugger",
      "tabs",
      "webNavigation"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  }
  